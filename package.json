{
  "name": "electron-starter",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "rebuild": "electron-rebuild",
    "test": "electron-mocha --compilers js:babel-register --recursive $(find test -name '*.spec.js') --verbose || true",
    "copy:fonts": "copy ./src/renderer/assets/fonts/**/* ./dist/renderer/assets/fonts/",
    "build:scss": "node-sass ./src/renderer/assets/styles/style.scss ./dist/renderer/assets/styles/style.css --output-style compressed",
    "watch:scss": "node-sass ./src/renderer/assets/styles/style.scss ./dist/renderer/assets/styles/style.css --source-map-embed --watch",
    "build:webpack": "webpack -p",
    "build:template": "pug src/renderer/**.pug --out ./dist/renderer --pretty",
    "create:packageJson": "gulp create:packageJson",
    "release:osx": "electron-packager ./dist --out=release --electron-version=1.6.2 --overwrite",
    "watch:webpack": "webpack --watch",
    "watch:template": "pug src/renderer/**.pug --out ./dist/renderer --pretty --watch",
    "server": "gulp server",
    "dev:build": "npm-run-all -s clean build:* create:packageJson",
    "dev:watch": "npm-run-all -p watch:* server",
    "dev": "npm-run-all -s dev:build dev:watch",
    "start": "npm-run-all dev",
    "build": "npm-run-all -s clean build:* create:packageJson release:osx",
    "clean": "rimraf ./dist"
  },
  "keywords": [
    "Electron"
  ],
  "dependencies": {
    "electron": "1.6.2",
    "normalize.css": "^7.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^4.1.2",
    "copy": "^0.3.0",
    "electron-connect": "^0.6.1",
    "electron-mocha": "^4.0.3",
    "electron-packager": "^8.7.0",
    "electron-rebuild": "^1.6.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "gulp": "^3.9.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "pug": "^2.0.0-rc.1",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.6.1",
    "webpack": "^2.3.2"
  }
}
