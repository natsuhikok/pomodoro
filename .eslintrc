{
  "extends": "airbnb",
  "globals": {
    "window": true,
    "describe": true,
    "it": true,
    "after": true,
    "before": true,
    "beforeEach": true,
    "afterEach": true,
  },
  "rules": {
    "no-multi-spaces": 0,
    "import/extensions": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-underscore-dangle": 1,
  }
}
